/**
 * Environment Configuration Template
 * Copy this to .env and customize for your environment
 */

# Application Settings
APP_NAME=ShadowRecon
APP_VERSION=3.0.0
NODE_ENV=development

# Webhook Configuration
WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_URL_HERE
WEBHOOK_TIMEOUT=30000

# Module Settings (true/false)
ENABLE_BROWSERS=true
ENABLE_DISCORD=true
ENABLE_CRYPTO=false
ENABLE_FILES=false
ENABLE_SYSTEM=true
ENABLE_INJECTION=false

# Security Settings
ENABLE_VM_DETECTION=false
ENABLE_SELF_DESTRUCT=false

# Upload Settings
MAX_UPLOAD_SIZE=7340032
UPLOAD_SERVICE=gofile

# Logging
LOG_LEVEL=INFO

# Paths (optional - will use system defaults if not set)
# TEMP_PATH=/path/to/temp
# STARTUP_PATH=/path/to/startup
# OUTPUT_PATH=./output